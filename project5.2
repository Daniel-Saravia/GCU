int main() {
  cout << "Hello World!\n";
  // Slips
  string myStrings[10];
  myStrings[0] = "DFG";
  myStrings[1] = "EFG";
  myStrings[2] = "DFFFFFG";
  myStrings[3] = "DFDFDFDFG";
  myStrings[4] = "DFEFFFFFG";
  // not Slips
  myStrings[5] = "DFEFF";
  myStrings[6] = "EFAHG";
  myStrings[7] = "DEFG";
  myStrings[8] = "DG";
  myStrings[9] = "EFFFFDG";
  for (int i = 0; i < 10; i++) {
    cout << isSlip(myStrings[i]) << endl;
  }

  // string myString = "DG";

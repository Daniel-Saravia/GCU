#include <iostream>
using namespace std;
bool isSlip(string str) {
  // check for D or E
  if (str[0] == 'D' || str[0] == 'E') {
    // check for F or F's
    for (int i = 1; i < str.length() && str[i] == 'F'; i++) {
      // end if G
      if (str[i + 1] == 'G') {
        return true;
        // recursion if i = D or E
      } else if (str[i + 1] == 'D' || str[i + 1] == 'E') {
        return isSlip(str.substr(i + 1));
      } else if (str[i + 1] == 'F') {
        continue;
      } else {
        return false;
      }
    }
  } else {
    return false;
  }
}

